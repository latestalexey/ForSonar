&НаКлиенте
Процедура ЗапроситьСертификатИПодписатьСФ()
	
	Если РаботаССерверомДиадок = Неопределено
		Или РаботаССерверомДиадок.АПИ = Неопределено 
		Или НЕ РаботаССерверомДиадок.Используется
		Тогда
		ИнициализироватьРаботуСДиадоком();
	КонецЕсли;
	
	ДоступныеСертификаты = Новый СписокЗначений;
	
	Certificates = РаботаССерверомДиадок.АПИ.GetPersonalCertificates();
	
	Для Сч = 0 По Certificates.Count()-1 Цикл
		
		ТекСертификат = Certificates.GetItem(Сч);
		
		Thumbprint = ТекСертификат.Thumbprint;
		Если 
			//ТекСертификат.IsQualifiedElectronicSignature //входим только по квалифицированным сертам
			НачалоДня(ТекСертификат.BeginDate)<=НачалоДня(ТекущаяДата())	//и не просроченным
			И КонецДня(ТекСертификат.EndDate)>=КонецДня(ТекущаяДата())
		Тогда	
			ПредставлениеСертификата = ТекСертификат.Name + "" / "" + ТекСертификат.OrganizationName + "" / до: "" + Формат(ТекСертификат.EndDate,""ДФ=dd.MM.yyyy"");
			ДоступныеСертификаты.Добавить(ТекСертификат.Thumbprint,ПредставлениеСертификата);//возможно, остальные поля тоже показать
		КонецЕсли;
		
	КонецЦикла;
	
	Если ДоступныеСертификаты.Количество()=0 Тогда
		ТекстПредупреждения=""Не обнаружены активные сертификаты для подписания"";
		Если Параметры.МодальностьЗапрещена Тогда 
			Выполнить(""ПоказатьПредупреждение(,ТекстПредупреждения,,""""EDI.Контур"""")"");
		Иначе
			Предупреждение(ТекстПредупреждения,,""EDI.Контур"");
		КонецЕсли;
		
		Возврат;
	КонецЕсли;
	
	ПараметрыФормы = Новый Структура;
	ПараметрыФормы.Вставить(""Режим"",		""ВыборИзСписка"");
	ПараметрыФормы.Вставить(""Заголовок"",	""Выберите сертификат для подписания"");
	ПараметрыФормы.Вставить(""СписокВыбора"",	ДоступныеСертификаты);
	
	Если Параметры.МодальностьЗапрещена Тогда 
		Выполнить(""ОткрытьФормуОбъектаМодально(ПутьКФормам + """"УниверсальныйДиалогУправляемая"""", ПараметрыФормы,""""ПослеВыбораСертификатаПользователем"""")"");//,ДополнительныеПараметры);
	Иначе
		ВыбранноеЗначение = ПолучитьФормуОбработки(""УниверсальныйДиалогУправляемая"",ПараметрыФормы).ОткрытьМодально();
		ПослеВыбораСертификатаПользователем(ВыбранноеЗначение);
	КонецЕсли;
	
КонецПроцедуры

&НаКлиенте
Процедура ПослеВыбораСертификатаПользователем(ВыбранныйЭлемент,Парам2=Неопределено) Экспорт
	//собственно подписание
	Если ВыбранныйЭлемент=Неопределено Тогда
		Возврат;
	КонецЕсли;
	
		
	//создадим соединение с Диадоком
	СоединениеДиадок = РаботаССерверомДиадок.АПИ.CreateConnectionByCertificate(ВыбранныйЭлемент.Значение);
	
	МассивСообщений = Новый Массив;
	Для Каждого Стр Из СчетаФактурыНаПодписание Цикл
		МассивСообщений.Добавить(Стр.Ссылка);
	КонецЦикла;
	
	//получим boxId и messageId
	СобратьТаблицуСчетовФактурНаПодписание(МассивСообщений);
	
	//и подписание
	КэшСоединений = Новый Соответствие;
	
	ПодписаниеУдалось = Ложь;
	
	Для Каждого ПодписываемыйДокумент Из СчетаФактурыНаПодписание Цикл
		ПриведенныйIDОрганизации = СтрЗаменить(ПодписываемыйДокумент.boxId,""-"","""");
		ТекущаяОрганизация = КэшСоединений.Получить(ПриведенныйIDОрганизации);
		Если ТекущаяОрганизация = Неопределено Тогда
			Попытка
				ТекущаяОрганизация = СоединениеДиадок.GetOrganizationById(ПриведенныйIDОрганизации+""@diadoc.ru"");
				КэшСоединений.Вставить(ПриведенныйIDОрганизации,ТекущаяОрганизация);
			Исключение
				Сообщить(""Сертификат не имеет доступа к организации либо документ уже подписан: ""+ПодписываемыйДокумент.ДокументПредставление);
				Продолжить;
			КонецПопытки;
		КонецЕсли;
		
		Результат = ТекущаяОрганизация.SendDraftAsync(ПодписываемыйДокумент.messageId);
		//ПодписываемыйДокумент.Вставить(""Результат"",Результат);
		Сообщить(""Подписан: ""+ПодписываемыйДокумент.ДокументПредставление);
		//тут можно дождаться результата
		ПодписаниеУдалось = Истина;
	КонецЦикла;
	
	ТекстПредупреждения="""";
	Если ПодписаниеУдалось Тогда
		ТекстПредупреждения=""Выполните обмен с сервером для обновления статусов сообщений"";
	Иначе
		ТекстПредупреждения=""Не удалось подписать счета-фактуры"";
	КонецЕсли;
	
	Если Параметры.МодальностьЗапрещена Тогда 
		Выполнить(""ПоказатьПредупреждение(,ТекстПредупреждения,,""""EDI.Контур"""")"");
	Иначе
		Предупреждение(ТекстПредупреждения,,""EDI.Контур"");
	КонецЕсли;
		
КонецПроцедуры



