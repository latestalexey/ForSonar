Процедура ПолучитьНастройкиССервера()
	
	НастройкиССервера = МодульОбъекта().ПрочитатьНастройкиССервера(Новый Структура(""Сервер,Порт,Логин,Пароль,ПассивныйРежим"",Сервер,Порт,Логин,Пароль,ПассивныйРежим));
	
	//пока ТС нет
